- content_for :stylesheets do
  = stylesheet_link_tag 'compiled/community_management.css', :media => 'screen, projection'
- content_for :javascripts do
  = javascript_include_tag 'community_management.js', :media => 'screen, projection'

%h1 Applicants

%ul#applicant-list.member-list
  - @canvas.canvas_applicants.each do |ca|
    %li.member-item{:data => {:user_id => ca.user_id} }
      .tools
        %a.approve{:href => "#"} Approve
        %a.disapprove{:href => "#"} Disapprove
        %a.ban{:href => "#"} Ban
      .name= ca.applicant.name
      .note= ca.note